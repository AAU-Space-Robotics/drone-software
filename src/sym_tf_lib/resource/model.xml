<robot_model>
    <model name="robot">
        <!-- Mass matrix -->
       <mass_matrix rows="2" cols="2">
            <name>M</name>
            <description>Mass Matrix</description>
            <entries>
                <m11>m1 * lc1**2 + I1 + I2 + m2 * (l1**2 + lc2**2 + 2 * l1 * lc2 * cos(theta2))</m11>
                <m12>m2 * (lc2**2 + l1 * lc2 * cos(theta2)) + I2</m12>
                <m21>m2 * (lc2**2 + l1 * lc2 * cos(theta2)) + I2</m21>
                <m22>m2 * lc2**2 + I2</m22>
            </entries>
            <parameters>
                <q>theta1 theta2</q>
            </parameters>
        </mass_matrix>


        <!-- Velocity terms (Coriolis/centripetal terms) -->
        <velocity_terms rows="2" cols="1">
            <name>C</name>
            <description>Coriolis and Centripetal Forces</description>
            <entries>
                <c1>-m2 * l1 * lc2 * sin(theta2) * w2**2 - 2 * m2 * l1 * lc2 * sin(theta2) * w1 * w2</c1>
                <c2>m2 * l1 * lc2 * sin(theta2) * w1**2</c2>
            </entries>
            <parameters>
                <q>theta1 theta2</q>
                <q_dot>w1 w2</q_dot>
            </parameters>
        </velocity_terms>


        <!-- Gravity vector -->
        <gravity_vector rows="2" cols="1">
            <name>G</name>
            <description>Gravity Vector</description>
            <entries>
                <g1>m1 * g * lc1 * cos(theta1) + m2 * g * (l1 * cos(theta1) + lc2 * cos(theta1 + theta2))</g1>
                <g2>m2 * g * lc2 * cos(theta1 + theta2)</g2>
            </entries>
            <parameters>
                <q>theta1 theta2</q>
            </parameters>
        </gravity_vector>


        <!-- Control input -->
        <control_inputs rows="2" cols="1">
            <name>u</name>
            <description>Control Input</description>
            <entries>
                <u1>tau1</u1>
                <u2>tau2</u2>
            </entries>
            <parameters>
                <q>tau1 tau2</q>
            </parameters>
        </control_inputs>

        
    </model>
</robot_model>
